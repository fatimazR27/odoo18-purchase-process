<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="seq_purchase_project" model="ir.sequence">
            <field name="name">Projet Code</field>
            <field name="code">purchase.process.project</field>
            <field name="prefix">PROJ/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="seq_purchase_chantier" model="ir.sequence">
            <field name="name">Chantier Code</field>
            <field name="code">purchase.process.chantier</field>
            <field name="prefix">CH/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="seq_demande_achat" model="ir.sequence">
            <field name="name">Demande Achat</field>
            <field name="code">purchase.process.demande.achat</field>
            <field name="prefix">DA/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>
