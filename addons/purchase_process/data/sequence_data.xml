<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Sequence for Project -->
        <record id="seq_purchase_process_project" model="ir.sequence">
            <field name="name">Project Sequence</field>
            <field name="code">purchase.process.project</field>
            <field name="prefix">PROJ/%(year)s/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequence for Chantier -->
        <record id="seq_purchase_process_chantier" model="ir.sequence">
            <field name="name">Chantier Sequence</field>
            <field name="code">purchase.process.chantier</field>
            <field name="prefix">CHANT/%(year)s/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequence for Demande Achat -->
        <record id="seq_purchase_process_demande_achat" model="ir.sequence">
            <field name="name">Demande Achat Sequence</field>
            <field name="code">purchase.process.demande.achat</field>
            <field name="prefix">DA/%(year)s/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>
